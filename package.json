{
  "name": "atul",
  "version": "0.0.1",
  "description": "workspace setup tool, mac, linux, etc",
  "author": "@kdavh",
  "bin": {
    "atul": "./bin/run"
  },
  "homepage": "https://github.com/kdavh/atul",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "kdavh/atul",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.19.1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.4",
    "open-editor": "^4.0.0",
    "sinon": "^14.0.1"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.4",
    "@types/chai": "^4",
    "@types/execa": "^2.0.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.68",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.8.4"
  },
  "oclif": {
    "bin": "atul",
    "dirname": "atul",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "additionalHelpFlags": [
      "-h"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "postbuild": "cp src/pkg/install.sh dist/pkg/install.sh",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0 <16"
  },
  "bugs": "https://github.com/dev/hello-world/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
